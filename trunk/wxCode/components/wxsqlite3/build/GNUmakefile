ifeq ($(SHARED),0)
../lib/gcc_$(__wxsqlite3_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsqlite3-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxsqlite3_lib  $(WXSQLITE3_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXSQLITE3_LIB_OBJECTS)
